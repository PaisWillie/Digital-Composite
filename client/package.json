{
  "name": "reactjs-vite-tailwindcss-boilerplate",
  "description": "This is a boilerplate build with Vite, React 18, TypeScript, Vitest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Jo√£o Paulo Moraes",
    "email": "joaopaulomoraes@outlook.com",
    "url": "https://github.com/joaopaulomoraes"
  },
  "bugs": {
    "url": "https://github.com/joaopaulomoraes/reactjs-vite-tailwindcss-boilerplate/issues",
    "email": "joaopaulomoraes@outlook.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint --fix src && pnpm format",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:setup": "playwright test tests/auth.setup.spec.ts --config=playwright.config.ts --headed",
    "test:run": "playwright test --config=playwright.config.ts --headed",
    "test:debug": "playwright test --config=playwright.config.ts --debug",
    "test:runnyc": "pnpm exec nyc --reporter=html --reporter=text-summary pnpm exec playwright test --config=playwright.config.ts --headed",
    "test:coverage": "pnpm exec nyc report --reporter=html --reporter=text-summary"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@udecode/cn": "^43.0.0",
    "antd": "^5.23.3",
    "chart.js": "^4.4.7",
    "dotenv": "^16.4.7",
    "fuse.js": "^7.1.0",
    "pnpm": "^9.15.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "react-select": "^5.10.0",
    "react-toastify": "^11.0.3",
    "uuid": "^11.0.5",
    "@playwright/test": "^1.41.1",
    "@types/node": "^20.11.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-istanbul": "^5.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.51.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "happy-dom": "^12.10.3",
    "nyc": "^17.1.0",
    "playwright": "^1.51.0",
    "postcss": "^8.4.47",
    "prettier": "3.1.1",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
